{
    "name": "redcode/twigjs-compiler",
    "description": "This library allows to compile twig templates to requireJS supported format",
    "keywords": ["twig", "twigjs", "requirejs", "js", "compiler", "template"],
    "type": "lib",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Reddikh",
            "email": "andrew@reddikh.com",
            "homepage": "http://reddikh.com"
        },
        {
            "name": "Alexander Savin",
            "email": "box.for.postman@gmail.com"
        },
        {
            "name": "Andrew Balakirev",
            "email": "balakirev.andrey@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "bin": ["bin/twigjs-compile"],
    "require": {
        "jms/twig-js": "^1.0",
        "symfony/console": "^2.7",
        "symfony/yaml": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "fabpot/php-cs-fixer": "^1.10"
    },
    "autoload": {
        "psr-0": { "RedCode\\TwigJs": "src/" }
    }
}
